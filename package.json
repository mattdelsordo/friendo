{
  "name": "friendo",
  "version": "1.1.3",
  "description": "Friendo: a virtual pal.",
  "main": "index.js",
  "repository": "https://github.com/mattdelsordo/friendo.git",
  "author": "Matt DelSordo",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "github-release-notes": "^0.17.0",
    "husky": "^0.14.3",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.17.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "scripts": {
    "build-dev": "NODE_ENV=development yarn dev-js && yarn dev-html && yarn dev-sass",
    "build-prod": "NODE_ENV=production yarn prod-js && yarn prod-html && yarn prod-sass",
    "copy-res": "cp -r res/. docs/",
    "dev-html": "pug -P src/ui/pages/index.pug -o docs/ && yarn copy-res",
    "dev-js": "webpack -d --progress",
    "dev-sass": "sass src/scss/main.scss docs/styles.css",
    "dev-server": "yarn copy-res & webpack-dev-server & pug -P src/ui/pages/ -o docs/ -w & sass --watch src/scss/main.scss docs/styles.css",
    "prod-html": "pug src/ui/pages/index.pug -o docs/ && yarn copy-res",
    "prod-js": "webpack -p",
    "prod-sass": "sass src/scss/main.scss docs/styles.css --style compressed",
    "release": "gren release --token=$FRIENDO_GREN_TOKEN",
    "test": "./scripts/test-vernum.sh && eslint src"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.0",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.2.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "tether": "^1.4.4"
  }
}
