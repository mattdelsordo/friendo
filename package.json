{
  "name": "friendo-dev",
  "version": "0.1.21",
  "description": "Dev-kit for Friendo.",
  "main": "index.js",
  "repository": "https://github.com/mattdelsordo/friendo-dev.git",
  "author": "Matt DelSordo",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.14.3",
    "pug-cli": "^1.0.0-alpha6",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "scripts": {
    "build-dev": "NODE_ENV=development yarn dev-js && yarn dev-html",
    "dev-html": "pug -P ui/index.pug -o docs/ && yarn copy-res",
    "dev-js": "webpack -d --progress",
    "build-prod": "NODE_ENV=production yarn prod-js && yarn prod-html",
    "prod-html": "pug ui/index.pug -o docs/ && yarn copy-res",
    "copy-res": "cp -r ui/resources/. docs/",
    "prod-js": "webpack -p",
    "dev-server": "yarn copy-res & webpack-dev-server & pug -P ui/index.pug -o docs/ -w",
    "test": "eslint src"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.0",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.2.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "tether": "^1.4.4"
  }
}
